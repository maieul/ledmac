    \ifnumberpstart%
      \ifsidepstartnum%
        \else%
         \thepstartL%
        \fi%
       \fi%
      \fi%
  \hsize=\Lcolwidth%
  \numberedpar@true%
  \iflabelpstart\protected@edef\@currentlabel%
       {\p@pstartL\thepstartL}\fi%
  \ifstrempty{#1}%
    {\csgdef{before@pstartL@\the\l@dnumpstartsL}{\at@every@pstart}}%
    {\csgdef{before@pstartL@\the\l@dnumpstartsL}{\noindent#1}}%
  }
